{
  "name": "llmatic",
  "version": "0.2.1",
  "description": "Use self-hosted LLMs with an OpenAI compatible API",
  "keywords": [
    "openai",
    "api",
    "llm",
    "llama"
  ],
  "repository": "git@github.com:fardjad/node-llmatic.git",
  "license": "MIT",
  "author": "Fardjad Davari <public@fardjad.com>",
  "bin": {
    "llmatic": "./cli/llmatic"
  },
  "files": [
    "README.md",
    "LICENSE",
    "*.mjs",
    "api.oas.yml",
    "public/",
    "patches/",
    "media/",
    "handlers/",
    "cli/",
    "!**/*.test.mjs"
  ],
  "scripts": {
    "fetch-openai-oas": "node ./scripts/fetch-openai-oas.mjs",
    "fix": "prettier --write . && eslint --ext .mjs --fix .",
    "postinstall": "patch-package",
    "lint": "eslint --ext .mjs .",
    "test": "node --test --test-reporter spec",
    "test:watch": "npm run test -- --watch ."
  },
  "dependencies": {
    "@fastify/static": "^6.10.1",
    "@inquirer/prompts": "^1.2.3",
    "@inquirer/select": "^1.1.7",
    "@llama-node/llama-cpp": "^0.1.5",
    "ajv": "^8.12.0",
    "awilix": "^8.0.1",
    "commander": "^10.0.1",
    "event-iterator": "^2.0.0",
    "fastify": "^4.17.0",
    "fastify-openapi-glue": "^4.1.4",
    "fastify-sse-v2": "^3.1.0",
    "glob": "^10.2.6",
    "llama-node": "^0.1.5",
    "nodejs-file-downloader": "^4.11.2",
    "nodemon": "^2.0.22",
    "patch-package": "^7.0.0",
    "swagger-ui-dist": "^4.18.3",
    "traverse": "^0.6.7",
    "valid-filename": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "prettier": "^2.8.8"
  }
}
